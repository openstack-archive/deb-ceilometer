msgid ""
"Invalid YAML syntax in Definitions file %(file)s at line: %(line)s, column: "
"%(column)s."
msgstr "定義檔 %(file)s 第 %(line)s 行第 %(column)s 列中的 YAML 語法無效。"

#, python-format
msgid "Invalid period %(period)s: %(err)s"
msgstr "期間 %(period)s 無效：%(err)s"

